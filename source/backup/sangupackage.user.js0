			row.find("td:eq(1)").css("background-color", user_data.colors.error);										}													// extra column with dodge time										if (incomingType == 'incAt') {											var dodgeTime = getTimeFromTW($("td:eq(2)", this).text());											row.find("td:last").before("<td class=dodgers>" + twDurationFormat(dodgeTime.totalSecs / 2 / 60) + "</td>");											amountOfAttacks++;										} else {											row.append("<td>&nbsp;</td>");										}													// dotted line after x hours no incomings										if (prevSendTime == 0 || (currentArrivalTime - prevSendTime) / 1000 / 60 > user_data.mainTagger.minutesWithoutAttacksDottedLine) {											if (prevSendTime != 0) {												row.find("td").css("border-top", "1px dotted black");											}																						prevSendTime = currentArrivalTime;										}																				// black line after each nightbonus										if (lastSend == 0 || currentArrivalTime > lastSend) {											if (lastSend != 0) {												row.find("td").css("border-top", "1px solid black");												firstNight = false;											}														lastSend = new Date(currentArrivalTime);											if (lastSend.getHours() >= world_config.nightbonus.till) {												lastSend.setDate(lastSend.getDate() + 1);												lastSend.setHours(world_config.nightbonus.from);												lastSend.setMinutes(0);												lastSend.setSeconds(0);											} else if (lastSend.getHours() < world_config.nightbonus.from) {												lastSend.setHours(world_config.nightbonus.from);												lastSend.setMinutes(0);												lastSend.setSeconds(0);											} else {												lastSend.setHours(world_config.nightbonus.till);												lastSend.setMinutes(0);												lastSend.setSeconds(0);											}										}													// Automatically select?										if (incomingType == "incAt") {											if (firstNight) {												var isDefaultDesc = false;												if (!isDefaultDesc) {								